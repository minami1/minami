*
	margin 0
	padding 0
body
	font-family Microsoft YaHei
.fabu-homepage
	width 100%
	background #1D1C1B
	.fabu-homepage-center
		width 100%
		height 135px
		color white
		position absolute
		bottom 15%
		display flex
		text-align center
		.fabu-homepage-center-left
			flex 1
			.fabu-homepage-left-img
				width 94px
				height 94px
				display inline-block
				background url(../img/house/i/publish_btn02@2x.png) center no-repeat
				background-size 100%
				margin-bottom 5px
		.fabu-homepage-center-right
			flex 1
			.fabu-homepage-right-img
				width 94px
				height 94px
				display inline-block
				background url(../img/house/i/publish_btn01@2x.png) center no-repeat
				background-size 100%
				margin-bottom 5px
	.fabu-homepage-bottom
		left 0
		right 0
		height 50px
		border-bottom 4px
		position absolute
		bottom 0
		background #242221
		line-height 50px
		text-align center
		.fabu-homepage-bottom-img
			width 14px
			height 14px
			background url(../img/house/i/publish_close_icon@2x.png) center no-repeat
			background-size 100%
			display inline-block
.fabu-needs
	background #EFEFF4
	display none
	height 100%
	width 100%
	.needs-top
		height 45px
		background white
		text-align center
		font-size 18px
		font-weight bold
		line-height 45px
		overflow hidden
		.needs-top-left
			font-weight 300
			font-size 14px
			float left
			margin-left 10px
		.needs-top-right
			font-weight 300
			font-size 14px
			float right
			margin-right 10px
			color #ED7139
	.needs-absolute
		position absolute
		background rgba(180,180,180,0.6)
		width 100%
		top 0
		display none
		height 100%
		z-index 10
		.absolute-center
			position absolute
			top 50%
			left 50%
			margin-left -132px
			margin-top -55px
			border-radius 10px
			width 275px
			height 110px
			background white
			text-align center
			.absolute-center-top
				height 65px
				line-height 65px
				font-size 14px
				border-bottom 1px solid #EBEBEE
			.absolute-center-bottom
				border-radius 0px 0px 10px 10px
				display flex
				height 45px
				line-height 45px
				.absolute-center-bottom-left
					border-radius 0px 0px 0px 10px
					flex 1
					border-right 1px solid #EBEBEE
					color #7E7E7E
				.absolute-center-bottom-right
					border-radius 0px 0px 10px 0px
					flex 1
					color #EA5404
	.fabu-absolute
		position absolute
		background #B2B2B2
		width 100%
		top 0
		display none
		height 100%
