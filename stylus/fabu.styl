.needs-contain
	margin 10px 25px
	.needs-contain-name
		input
			font-size 18px
			height 45px
			width 100%
			border none
			outline none
			border-radius 5px
			padding-left 10px
	.needs-contain-number
		margin-top 10px
		input
			font-size 18px
			height 45px
			width 100%
			border none
			outline none
			border-radius 5px
			padding-left 10px
	.needs-contain-brief
		margin-top 10px
		textarea
			font-size 18px
			width 100%
			border none
			outline-color #F4AA83
			outline-width 1px
			border-radius 5px
			padding-left 10px
			padding-top 10px
			resize none
.fabu-housing
	height 100%
	width 100%
	display none
	background #EFEFF4
	.housing-top
		height 45px
		background white
		text-align center
		font-size 18px
		font-weight bold
		line-height 45px
		overflow hidden
		position relative
		.housing-top-left
			position absolute
			font-weight 500
			font-size 16px
			left 10px
	.housing-add
		width 90%
		margin 10px auto
		display flex
		.housing-add-left
			flex 1
			border-radius 5px
			background white
			height 30px
			line-height 25px
			position relative
			.housing-add-left-img
				width 13px
				height 13px
				margin-left 10px
				display inline-block
				background url(../img/house/i/content_search_icon@2x.png) center no-repeat
				background-size 100%
				vertical-align text-bottom
			.housing-add-left-txt
				display inline-block
				input
					border none
					outline none
					font-size 12px
				.housing-add-position-img
					display inline-block
					width 13px
					height 13px
					background url(../img/house/i/content_delet_icon@2x.png) center no-repeat
					background-size 100%
					position absolute
					top 50%
					margin-top -6px
					right 2%
					display none
		.housing-add-right
			line-height 25px
			.housing-add-right-img
				width 12px
				height 12px
				display inline-block
				background url(../img/house/i/content_plus_icon@2x.png) center no-repeat
				margin-left 10px
				vertical-align text-bottom
			.housing-add-right-txt
				color #525253
				font-size 12px
	.housing-contain
		.housing-contain-top
			background white
			height 260px
			padding 10px
			.housing-contain-title
				font-size 14px
				.housing-contain-title-img
					width 5px
					height 5px
					border-radius 50%
					background #C0EA4F
					display inline-block
			.housing-contain-content
				list-style none
				font-size 12px
				padding-left 25px
				.housing-contain-content-li
					padding-top 5px
					overflow hidden
					display flex
					margin-top 5px
					.housing-contain-content-left
						color #838383
					.housing-contain-content-right
						display inline-block
						flex 1
						border-bottom 1px solid #EDEDEF
						input
							border none
							outline none
	.housing-contain-news
		margin-top 10px
		padding 10px
		background white
		.housing-contain-news-title
			font-size 14px
			.housing-contain-news-title-img
				width 5px
				height 5px
				border-radius 50%
				background #4ACDE9
				display inline-block
			.housing-news-title-bottom
				padding-left 25px
				padding-bottom 10px
				margin-top 5px
				color #898989
				font-size 12px
				border-bottom 1px solid #F2F2F2
				.housing-news-title-bottom-ul
					list-style none
					color #909090
					margin-left 25px
					.housing-news-title-bottom-li
						display inline-block
						margin-left 5px
						margin-top 5px
						.housing-news-title-bottom-li-img
							width 8px
							height 8px
							display inline-block
							background url(../img/house/i/content_nor_icon@2x.png) center no-repeat
							background-size 100%
							position relative
							.housing-news-title-bottom-li-img-radius
								width 4px
								height 4px
								display inline-block
								position absolute
								top 50%
								margin-top -2px
								left 50%
								margin-left -2px
								background url(../img/house/i/content_press_icon@2x.png) center no-repeat
								background-size 100%
		.housing-contain-news-centent
			font-size 12px
			color #C4C4C4
			margin-top 10px
			.housing-news-centent-li
				display flex
				margin-top 10px
				.housing-news-centent-li-left
					display inline-block
					margin-right 10px
					line-height 30px
				.housing-news-centent-li-right
					flex 1
					display flex
					margin-right 5px
					.housing-news-right-left
						flex 1
						border none
						position relative
						textarea
							resize none
							border none
							outline none
							padding 5px 0px 5px 5px
							width 100%
							background #EFEFF4
							border-radius 5px
						input
							border none
							outline none
							background #EFEFF4
							width 100%
							height 30px
							padding-left 5px
							border-radius 5px
						.housing-news-input
							display inline-block
							width 8px
							height 15px
							position absolute
							top 50%
							margin-top -7px
							right 2%
							background url(../img/house/i/content_list_right_icon@2x.png) center no-repeat
							background-size 100%
							transform rotate(90deg)
					.housing-news-right-right
						flex 1
						padding-left 15px
						line-height 30px
						color #484848
						input
							border none
							outline none
							background #EFEFF4
							width 100%
							height 30px
							padding-left 5px
							border-radius 5px
						.housing-news-title-bottom-li-img
							width 8px
							height 8px
							display inline-block
							background url(../img/house/i/content_nor_icon@2x.png) center no-repeat
							background-size 100%
							position relative
							.housing-news-title-bottom-li-img-radius
								width 4px
								height 4px
								display inline-block
								position absolute
								top 50%
								margin-top -2px
								left 50%
								margin-left -2px
								background url(../img/house/i/content_press_icon@2x.png) center no-repeat
								background-size 100%
			.housing-news-add
				width 75px
				height 75px
				background #D4D4D9
				text-align center
				position relative
				color #989898
				.housing-news-add-sp
					position absolute
					top 50%
					margin-top -18px
					.housing-news-add-img
						width 24px
						height 24px
						display inline-block
						background url(../img/house/i/content_plus_icon@2x.png) center no-repeat
						background-size 100%
						.housing-news-add-input
							border none
							opacity 0
					.housing-news-add-bottom
						display inline-block
	.housing-contain-end
		height 60px
		background white
		margin-top 10px
		text-align center
		.housing-contain-end-btn
			display inline-block
			width 95%
			margin auto
			background #EA5404
			color #FDF2EF
			height 40px
			line-height 40px
			font-size 16px
			border none
			border-radius 5px
			margin-top 10px