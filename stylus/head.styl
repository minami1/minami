*
	margin 0
	padding 0
body
	font-family Microsoft YaHei
	transition all 1s
	background #EFEFF4
.page-slide
	position relative
	left 0px
	transition 1s
a
	text-decoration none
.top
		color #ED6E2A
		text-align center
		font-size 20px
		height 45px
		line-height 45px
		position relative
		background white
		.dl-top-back
			width 100px
			height 45px
			position absolute
			left 10px
			background url(../img/house/geren/nav_back_icon@2x.png) left center no-repeat
			background-size 10%
.page-last
	height 50px
	background white
	position fixed
	bottom 0
	width 100%
	ul
		list-style none
		display flex
		width 95%
		margin 0 auto
		li
			flex 1
			text-align center
			margin-top 5px
			.page-last-img1
				width 100%
				height 21px
				background url(../img/house/i/bottom_nor_icon04@2x.png) center no-repeat
				display block
				background-size 21px 21px
			.page-last-img2
				width 100%
				height 21px
				background url(../img/house/i/bottom_nor_icon03@2x.png) center no-repeat
				display block
				background-size 21px 21px
			.page-last-img3
				width 45px
				height 40px
				position relative
				left 50%
				margin-left -22px
				background #EA5404 url(../img/house/i/bottom_icon05@2x.png) center right 0px no-repeat
				display block
				background-size 100% 100%
			.page-last-img4
				width 100%
				height 21px
				background url(../img/house/i/bottom_nor_icon02@2x.png) center no-repeat
				display block
				background-size 21px 21px
				position relative
				.page-last-img4-box
					border-radius 50%
					width 15px
					height 15px
					font-size 12px
					text-align center
					background #EA5404
					position absolute
					top -5px
					left 50%
					color white
			.page-last-img5
				width 100%
				height 21px
				background url(../img/house/i/bottom_press_icon01@2x.png) center no-repeat
				display block
				background-size 21px 21px
				position relative
			.page-last-text
				font-size 14px